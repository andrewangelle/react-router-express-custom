{
  "name": "react-router-express-custom",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:app": "react-router build",
    "clean": "rm -rf ./build && rm -rf ./dist",
    "build": "pnpm clean && pnpm build:app && pnpm build:server",
    "dev": "nodemon --exec vite-node server/app.ts",
    "start": "cross-env NODE_ENV=production node build/app.js",
    "typecheck": "react-router typegen && tsc",
    "lint:fix": "pnpm exec biome check . --write",
    "lint:check": "pnpm exec biome check .",
    "format": "pnpm exec biome format . --write",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.9.4",
    "@react-router/node": "^7.9.4",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "isbot": "^5.1.11",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@react-router/dev": "^7.9.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-node": "^3.2.4",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
